{
  "name": "workshop-protactor",
  "version": "1.0.0",
  "description": "This is a Workshop about Protactor",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run lint && npm run clean && tsc",
    "test": "npm run test:headless",
    "test:local": "npm run build && protractor dist/protractor/local.config.js",
    "test:headless": "npm run build && protractor dist/protractor/headless.config.js",
    "webdriver:update": "webdriver-manager update --gecko false",
    "lint": "tslint --project tsconfig.json protractor/**/*.ts test/**/*.ts src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kliver98/protractor-workshop-2020.git"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "protactor"
  ],
  "author": "Kliver Daniel <kliver1998@gmail.com> (kliver98)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kliver98/protractor-workshop-2020/issues"
  },
  "homepage": "https://github.com/kliver98/protractor-workshop-2020#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.17.4",
    "jasmine-spec-reporter": "^7.0.0",
    "protractor": "^7.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.10"
  }
}
